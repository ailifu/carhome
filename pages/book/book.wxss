/* pages/book/book.wxss */
/* pages/addcar/addcar.wxss */
page {
  background-color: #f8f8f8;
  /*checkbox选中后样式  */
  /*checkbox选中后图标样式  */
}
page ocr-navigator {
  height: 100%;
  margin: 0;
  padding: 0;
}
page .cartips {
  background-color: #E9D4D5;
  color: #E93D30;
  height: 70rpx;
  width: 750rpx;
  line-height: 70rpx;
  text-align: center;
  margin-bottom: 40rpx;
}
page .carmessage {
  width: 690rpx;
  margin: 0 auto 30rpx;
  background-color: #f8f8f8;
  box-shadow: 0 0 20rpx #bbb;
  border-radius: 15rpx;
  padding: 30rpx 0 0 0;
}
page .carmessage .cartitle {
  font-weight: bold;
  color: #565656;
  font-size: 30rpx;
  line-height: 30rpx;
  margin: 0 40rpx;
}
page .carmessage .cartitle::before {
  background-color: #F00;
  width: 5rpx;
  height: 30rpx;
  content: '';
  display: block;
  float: left;
  margin-right: 30rpx;
}
page .carmessage .drivermessage {
  display: flex;
  margin: 0 40rpx;
  border-bottom: 1px solid #dbdbdb;
}
page .carmessage .drivermessage .message_label {
  flex: 2;
  color: #A7A7A7;
  height: 80rpx;
  line-height: 80rpx;
  text-indent: 20rpx;
}
page .carmessage .drivermessage .weui-cell {
  flex: 5;
  line-height: 80rpx;
}
page .carmessage .drivermessage .weui-cell input {
  color: #696969;
  line-height: 80rpx;
  height: 80rpx;
}
page .carmessage .drivermessage .weui-cell picker {
  color: #525252;
  line-height: 80rpx;
  height: 80rpx;
}
page .carmessage .drivermessage:last-child {
  border: none;
}
page .steps {
  display: flex;
  flex-flow: row warp;
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 30rpx;
  /* justify-content:space-around */
}
page .steps image {
  width: 100%;
}
page radio .wx-radio-input {
  width: 30rpx;
  height: 30rpx;
  margin-right: 5rpx;
}
page radio .wx-radio-input.wx-radio-input-checked {
  background-color: #2559A0;
}
page .cu-bar {
  display: flex;
}
page .carbody {
  margin-bottom: 100rpx;
}
page .bookfoot {
  height: 88rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  background-color: #747474;
}
page .bookfoot .bookleft {
  flex: 5;
  display: flex;
}
page .bookfoot .bookleft image {
  width: 78rpx;
  height: 78rpx;
  position: absolute;
  top: -20rpx;
  padding: 5rpx;
  margin-left: 30rpx;
  background-color: #4a99d8;
  border-radius: 50%;
  border: 20rpx solid #747474;
}
page .bookfoot .bookleft .bookprice {
  padding-left: 150rpx;
  color: #fff;
  font-size: 36rpx;
  line-height: 88rpx;
}
page .bookfoot .bookright {
  flex: 3;
}
page .bookfoot .bookright button {
  width: 100%;
  border-radius: none;
}
page checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
}
page checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #2559A0;
  border-color: #2559A0;
}
page checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
